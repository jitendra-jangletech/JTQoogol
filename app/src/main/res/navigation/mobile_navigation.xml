<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.jangletech.qoogol.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" >
    </fragment>

    <fragment
        android:id="@+id/nav_about"
        android:name="com.jangletech.qoogol.ui.about.AboutFragment"
        android:label="@string/about"
        tools:layout="@layout/fragment_about" >
    </fragment>


    <fragment
        android:id="@+id/nav_learning"
        android:name="com.jangletech.qoogol.ui.learning.LearningFragment"
        android:label="@string/menu_learning"
        app:enterAnim="@anim/enter_from_left"
        tools:layout="@layout/learning_fragment" >
        <action
            android:id="@+id/action_nav_learning_to_nav_learning_details"
            app:destination="@id/nav_learning_details" />
        <action
            android:id="@+id/action_nav_learning_to_nav_comments"
            app:destination="@id/nav_comments" />
    </fragment>

    <fragment
        android:id="@+id/nav_learning_details"
        android:name="com.jangletech.qoogol.ui.learning.LearningDetailsFragment"
        android:label="@string/menu_learning_details"
        app:enterAnim="@anim/enter_from_left"
        tools:layout="@layout/fragment_learning_details" >
    </fragment>



    <fragment
        android:id="@+id/nav_recent_test"
        android:name="com.jangletech.qoogol.ui.test.recent_test.RecentTestFragment"
        android:label="@string/recent_test"
        app:enterAnim="@anim/enter_from_left"
        tools:layout="@layout/fragment_test_my" >
    </fragment>

    <fragment
        android:id="@+id/nav_comments"
        android:name="com.jangletech.qoogol.ui.learning.CommentFragment"
        android:label="@string/comments"
        app:enterAnim="@anim/enter_from_left"
        tools:layout="@layout/comment_view" >
    </fragment>



    <fragment
        android:id="@+id/nav_test_attempt_history"
        android:name="com.jangletech.qoogol.ui.test_attempt_history.TestAttemptHistoryFragment"
        android:label="@string/menu_test_attempt"
        tools:layout="@layout/fragment_test_attempt_history" >
    </fragment>

    <fragment
        android:id="@+id/nav_doubts"
        android:name="com.jangletech.qoogol.ui.doubts.DoubtsFragment"
        android:label="@string/menu_ask_doubts"
        tools:layout="@layout/doubts_fragment" >
    </fragment>

    <fragment
        android:id="@+id/nav_test_my"
        android:name="com.jangletech.qoogol.ui.test.my_test.MyTestFragment"
        android:label="@string/menu_test_practice"
        tools:layout="@layout/fragment_test_my" >
        <action
            android:id="@+id/action_nav_test_my_to_nav_comments"
            app:destination="@id/nav_comments" />
    </fragment>

    <fragment
        android:id="@+id/nav_test_popular"
        android:name="com.jangletech.qoogol.ui.test.popular_test.TestPopularFragment"
        android:label="@string/menu_test_popular"
        tools:layout="@layout/fragment_test_popular" />


    <fragment
        android:id="@+id/nav_shared_with_you"
        android:name="com.jangletech.qoogol.ui.test.recent_test.RecentTestFragment"
        android:label="@string/menu_test_shared_with_you"
        tools:layout="@layout/fragment_recent_test" />

    <fragment
        android:id="@+id/nav_shared_by_you"
        android:name="com.jangletech.qoogol.ui.test.shared_by_you.TestSharedByYouFragment"
        android:label="@string/menu_test_shared_by_you"
        tools:layout="@layout/fragment_test_shared_by_you" />


    <fragment
        android:id="@+id/nav_notifications"
        android:name="com.jangletech.qoogol.ui.notifications.NotificationsFragment"
        android:label="@string/menu_notifications"
        tools:layout="@layout/fragment_notifications" />


    <fragment
        android:id="@+id/nav_syllabus"
        android:name="com.jangletech.qoogol.ui.syllabus.SyllabusFragment"
        android:label="@string/syllabus"
        tools:layout="@layout/fragment_syllabus" />

    <fragment
        android:id="@+id/nav_settings"
        android:name="com.jangletech.qoogol.ui.settings.SettingsFragment"
        android:label="@string/menu_settings"
        tools:layout="@layout/settings_fragment" />

    //Edit Profile
    <fragment
        android:id="@+id/nav_edit_profile"
        android:name="com.jangletech.qoogol.ui.edit_profile.EditProfileFragment"
        android:label="@string/menu_edit_profile"
        tools:layout="@layout/fragment_edit_profile" />

    <fragment
        android:id="@+id/nav_faq"
        android:name="com.jangletech.qoogol.ui.faq.FaqFragment"
        android:label="@string/faq"
        tools:layout="@layout/faq_fragment" />

    <fragment
        android:id="@+id/nav_faq_details"
        android:name="com.jangletech.qoogol.ui.faq.FaqTopicDetailsFragment"
        android:label="@string/faq"
        tools:layout="@layout/fragment_faq_topic_details" />

    //Test Details
    <fragment
        android:id="@+id/nav_test_details"
        android:name="com.jangletech.qoogol.ui.test_details.TestDetailsFragment"
        android:label="@string/menu_test_details"
        tools:layout="@layout/test_details_fragment" />

    //Favourite Test
    <fragment
    android:id="@+id/nav_fav_test"
    android:name="com.jangletech.qoogol.ui.test.favourite.TestFavouriteFragment"
    android:label="@string/favourite_test"
    tools:layout="@layout/test_favourite_fragment" />

    <fragment
        android:id="@+id/nav_fav"
        android:name="com.jangletech.qoogol.ui.favourite.FavouriteFragment"
        android:label="@string/menu_fav"
        tools:layout="@layout/favourite_fragment" />

    <fragment
        android:id="@+id/nav_test_filter"
        android:name="com.jangletech.qoogol.ui.test.filter.TestFilterFragment"
        android:label="@string/filters"
        tools:layout="@layout/test_filter_fragment" />

    <fragment
        android:id="@+id/nav_share"
        android:name="com.jangletech.qoogol.ui.learning.ShareFragment"
        android:label="@string/share"
        tools:layout="@layout/fragment_share" />

    <fragment
        android:id="@+id/nav_connections"
        android:name="com.jangletech.qoogol.ui.connections.ConnectionsFragment"
        android:label="@string/connections"
        tools:layout="@layout/fragment_connections" />

    <fragment
        android:id="@+id/nav_requests"
        android:name="com.jangletech.qoogol.ui.connections.PendingRequestsFragment"
        android:label="@string/pending_req"
        tools:layout="@layout/fragment_pending_requests" />

    <fragment
        android:id="@+id/nav_blocked_connections"
        android:name="com.jangletech.qoogol.ui.connections.BlockedFragment"
        android:label="@string/blockedlist"
        tools:layout="@layout/fragment_blocked" />

    <fragment
        android:id="@+id/nav_import_contacts"
        android:name="com.jangletech.qoogol.ui.usercontacts.ContactListFragment"
        android:label="@string/importContacts"
        tools:layout="@layout/contact_list_fragment" />

    <fragment
        android:id="@+id/saved_questions"
        android:name="com.jangletech.qoogol.ui.saved.SavedQueFragment"
        android:label="@string/saved_questions"
        tools:layout="@layout/saved_fragment" />

    <fragment
        android:id="@+id/fav_questions"
        android:name="com.jangletech.qoogol.ui.favourite.FavQueFragment"
        android:label="@string/questions"
        tools:layout="@layout/learning_fragment" />

</navigation>