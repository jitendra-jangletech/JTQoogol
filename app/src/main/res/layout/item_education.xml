    <com.google.android.material.card.MaterialCardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorWhite"
        android:elevation="@dimen/_40sdp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:padding="@dimen/_10sdp"
            android:layout_height="wrap_content">

            <ImageView
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            android:src="@drawable/ic_delete"
            android:layout_width="wrap_content"
            android:id="@+id/delete"
            android:layout_height="wrap_content"/>


            <ImageView
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/delete"
                android:src="@drawable/ic_edit"
                android:layout_width="wrap_content"
                android:id="@+id/edit"
                android:layout_height="wrap_content"/>

            <TextView
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toLeftOf="@+id/edit"
                android:id="@+id/course_textview"
                android:text="Course"
                android:textSize="@dimen/_12ssp"
                android:padding="@dimen/_3sdp"
                android:textColor="@color/colorBlack"
                android:layout_width="0dp"
                android:layout_height="wrap_content"/>

            <TextView
                android:layout_marginTop="@dimen/_2sdp"
                app:layout_constraintTop_toBottomOf="@+id/course_textview"
                app:layout_constraintLeft_toLeftOf="parent"
                android:id="@+id/degree_textview"
                android:text="Degree"
                android:textSize="@dimen/_12ssp"
                android:padding="@dimen/_3sdp"
                android:textColor="@color/colorBlack"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

            <TextView
                android:layout_marginTop="@dimen/_2sdp"
                app:layout_constraintTop_toBottomOf="@+id/degree_textview"
                app:layout_constraintLeft_toLeftOf="parent"
                android:id="@+id/date_textview"
                android:text="Date"
                android:textSize="@dimen/_12ssp"
                android:padding="@dimen/_3sdp"
                android:textColor="@color/colorBlack"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"/>

        </androidx.constraintlayout.widget.ConstraintLayout>

